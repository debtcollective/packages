{
  "name": "@debtcollective/union-component",
  "version": "1.4.4",
  "description": "",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage --verbose --color",
    "type-check": "tsc --project tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint",
    "build": "rm -rf ./build && microbundle --jsx React.createElement",
    "dev": "microbundle watch",
    "start": "python -m SimpleHTTPServer 8000"
  },
  "source": "src/index.ts",
  "main": "build/index.js",
  "exports": "build/index.modern.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.9.0",
    "@xstate/react": "^0.8.1",
    "classnames": "^2.2.6",
    "react-phone-number-input": "^3.1.20",
    "styled-components": "5.2.1",
    "twin.macro": "^1.8.1",
    "xstate": "^4.13.0"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "tailwindcss": ">=1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "^12.1.4",
    "@types/classnames": "^2.2.10",
    "@types/eslint": "7.2.10",
    "@types/faker": "^5.1.0",
    "@types/node": "14.14.31",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-phone-number-input": "^3.0.6",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-gatsby": "^0.5.8",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "7.26.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "10.5.4",
    "microbundle": "0.13.0",
    "prettier": "2.2.1",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "17.4.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "tailwindcss": "^1.7.0",
    "ts-jest": "^26.3.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "chore",
          "ci",
          "dev",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test",
          "build"
        ]
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "bash -c tsc --noEmit"
    ],
    "*.{md}": [
      "prettier --write"
    ],
    "*.scss": [
      "stylefmt",
      "stylelint --syntax scss"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debtcollective/packages.git"
  },
  "keywords": [
    "membership"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/debtcollective/packages/issues"
  },
  "homepage": "https://github.com/debtcollective/packages#readme"
}
